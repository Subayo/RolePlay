{% embed "@green/templates/table-panel.html.twig" %}
    {% block table_panel_title %}{{ 'label.soundPlayeds'|trans }}{% endblock %}
    {% block table_panel_btn %}
        <a href="{{ path('appbundle_soundPlayed_add') }}" type="button" class="btn bg-teal btn-xs btn-labeled"><b><i class="icon-plus2"></i></b> {{ 'soundPlayed.label.create'|trans }}</a>
    {% endblock %}
    {% block table_panel_th %}
        <th>#</th>
    {% endblock %}
    {% block table_panel_tr %}
        {% for soundPlayed in entity.soundPlayeds %}
            <tr>
                <td>{{ entity.session }}</td>
            </tr>
        {% endfor %}
    {% endblock %}
{% endembed %}