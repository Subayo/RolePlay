<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Role Play</title>

    <!-- Global stylesheets -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet"
          type="text/css">
    <link href="{{ asset('themes/green/css/icons/icomoon/styles.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('assets/vendor/icomoon-bower/style.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('themes/green/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('themes/green/css/core.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('themes/green/css/components.min.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('themes/green/css/colors.min.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('assets/vendor/pnotify/dist/pnotify.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset('themes/green/css/style.css') }}" rel="stylesheet" type="text/css">
    {% block core_stylesheet %}{% endblock %}
    {% block inline_stylesheet %}{% endblock %}
    <!-- /global stylesheets -->
</head>

<body class="{% block class_body %}{% endblock %} pace-done">
{% for type, messages in app.session.flashbag.all() %}
    {% for message in messages %}
        <div class="notify-session" data-type="{{ type }}" data-message="{{ message }}"></div>
    {% endfor %}
{% endfor %}
<!-- Main navbar -->
<div class="navbar navbar-inverse">
    <div class="navbar-header">
        <ul class="nav navbar-nav pull-right visible-xs-block">
            <li><a data-toggle="collapse" data-target="#navbar-mobile"><i class="icon-tree5"></i></a></li>
            <li><a class="sidebar-mobile-main-toggle"><i class="icon-paragraph-justify3"></i></a></li>
            <li><a class="sidebar-mobile-detached-toggle"><i class="icon-grid7"></i></a></li>
        </ul>
    </div>

    <div class="navbar-collapse collapse" id="navbar-mobile">
        <ul class="nav navbar-nav">
            <li><a class="sidebar-control sidebar-main-toggle hidden-xs"><i class="icon-paragraph-justify3"></i></a>
            </li>
        </ul>
    </div>
</div>
<!-- /main navbar -->


<!-- Page container -->
<div class="page-container">

    <!-- Page content -->
    <div class="page-content">

        <!-- Main sidebar -->
        <div class="sidebar sidebar-main">
            <div class="sidebar-content">
                {% include "@green/partials/sidebar.html.twig" %}
            </div>
        </div>
        <!-- /main sidebar -->


        <!-- Main content -->
        <div class="content-wrapper">

            <!-- Page header -->
            <div class="page-header">
                <div class="page-header-content">
                    <div class="page-title">
                        <h4><i class="icon-arrow-left52 position-left"></i> {% block title_page %}<span class="text-semibold">{{ 'label.home'|trans }}</span>{% endblock %}</h4>
                    </div>

                    <div class="heading-elements">
                        {% block heading_elements %}{% endblock heading_elements %}
                    </div>
                </div>

                <div class="breadcrumb-line">
                    <ul class="breadcrumb">
                        <li><a href="{{ path('app_home_index') }}"><i class="icon-home2 position-left"></i> {{ 'label.home'|trans }}</a></li>
                        {% block breadcrumb %}
                        {% endblock breadcrumb %}
                    </ul>
                </div>
            </div>
            <!-- /page header -->


            <!-- Content area -->
            <div class="content">

                {% block main_container %}
                {% endblock %}

                <!-- Footer -->
                <div class="footer text-muted">
                    &copy; 2016. <a href="http://www.cartelmatic.com/" target="_blank">Cartelmatic</a>
                </div>
                <!-- /footer -->

            </div>
            <!-- /content area -->

        </div>
        <!-- /main content -->

    </div>
    <!-- /page content -->

</div>
<!-- /page container -->
<!-- Core JS files -->
<script type="text/javascript" src="{{ asset('assets/vendor/PACE/pace.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/vendor/jquery/dist/jquery.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/vendor/blockUI/jquery.blockUI.js') }}"></script>
<script type="text/javascript" src="{{ asset('themes/green/js/plugins/nicescroll.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('assets/vendor/pnotify/dist/pnotify.js') }}"></script>
{% block core_js %}{% endblock %}
<!-- /core JS files -->

<!-- Theme JS files -->
<script type="text/javascript" src="{{ asset('themes/green/js/core/app.js') }}"></script>
<script type="text/javascript" src="{{ asset('themes/green/js/app.js') }}"></script>
<script type="text/javascript" src="{{ asset('themes/green/js/layout_fixed_custom.js') }}"></script>
{% block theme_js %}{% endblock %}
{% block inline_js %}{% endblock %}
<!-- /theme JS files -->
</body>
</html>
